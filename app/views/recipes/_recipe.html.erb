<nav class ="box">
  <ul>
    <li><%= recipe.title %></li>
    <li> <%= link_to image_tag(recipe.recipe_image.thumb.url), recipe.recipe_image.medium.url %></li>
    <li><%= link_to("Show #{recipe.title}", recipe_path(recipe)) %></li>
    <li><%= link_to("Edit Recipe", edit_recipe_path(recipe)) %></li>
    <li><%= button_to('Delete Recipe', recipe_path(recipe), method: :delete, data:{confirm: "Delete Them? Are you sure?"}) %></li>
    
  </ul>
</nav>